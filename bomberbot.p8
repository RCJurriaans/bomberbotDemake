pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- picobot
-- studio krok
-- robrecht jurriaans
-- demake of bomberbot 

--globals
score=0
screen={}
screen.w=127
screen.h=127

states = { 
 splash = 0,
 program = 1,
 running = 2,
 completed = 3
}

state = states.splash

function init()		
end

function _update()
 if state == states.splash then
  update_splash()
 elseif state == states.program then
  update_program()
 elseif state == states.running then
  update_running()
 elseif state == states.completed then
  update_completed()
 end
end

function _draw()
 cls()
 if state == states.splash then
  draw_splash()
 elseif state == states.program then
  draw_program()
 elseif state == states.running then
  draw_running()
 elseif state == states.completed then
  draw_completed()
 end
end

-- on title screen

function update_splash()
 -- get input
 input_splash()
end

function input_splash()
 if btn(4) or btn(5) then
  state = states.program
 end

end

function draw_splash()
 local titletxt = "picobot"
 local starttxt = "press z to start"
 rectfill(0,0,screen.w, screen.h, 3)
 print(titletxt, hcenter(titletxt), screen.h/4, 10)
 print(starttxt, hcenter(starttxt), (screen.h/4)+(screen.h/2),7) 

 spr(0, 60, 60)
end


-- programming picobot
crsr_pos = {x=0, y=0, w=8, h=8, b=0, bl=20}

instructions = {}
actions = {'u', 'd', 'l', 'r', 'tl', 'tr', 'f', 'h', 'lo', 'lc', 'co', 'cc', 'f1', 'f2'}

function update_program()
 -- get input
 input_program()
 --update cursor
 crsr_pos.b += 1
 if crsr_pos.b>crsr_pos.bl then
  crsr_pos.b=0
 end
end

function input_program()
 if(btnp(0)) then
  movecrsr(-1, 0)
 elseif(btnp(1)) then
  movecrsr(1, 0)
 elseif(btnp(2)) then
  movecrsr(0, -1)
 elseif(btnp(3)) then
  movecrsr(0, 1)
 elseif(btnp(4)) then
  program_okay()
 elseif(btnp(5)) then

 end
end

function program_okay()
 if in_instruction() then
  add_instruction()
 elseif in_program() then

 end
end

function in_instruction()
 local x = crsr_pos.x
 local y = crsr_pos.y
 if x<2 and y<=6 then
  return true
 end
 return false
end


function add_instruction()
 local i = (crsr_pos.y * 2) + crsr_pos.x +1
 if(#instructions<20) then
  instructions[#instructions+1] = i
 end

end

function in_program()

end

function movecrsr(dx, dy)
 new_pos = {x=crsr_pos.x+dx, y=crsr_pos.y+dy, w=crsr_pos.w, h=crsr_pos.h, b=crsr_pos.b, bl=crsr_pos.bl}
 if isvalidpos(new_pos) then
  crsr_pos = new_pos
 end
end

function isvalidpos(pos) 
 if pos.x<0 or pos.y<0 then
  return false
 end

 return true
end

function draw_program()
 -- draw background
 -- draw instruction panel
 rectfill(0,0,16, 80, 5)
 map(0,0, 0, 0, 16, 16)
 -- draw map

 -- draw program panel
 rectfill(0, 81,127, 97, 12)
 rectfill(32,81,127, 97, 5)
 print('main', 2, 83,7)
 for i = 1,#instructions do
  draw_instruction(instructions[i], i)
 end

 -- draw instruction panel

 -- draw cursor
 if crsr_pos.b < (crsr_pos.bl/2) then
  rect(crsr_pos.x*8, crsr_pos.y*8, 8*crsr_pos.x+crsr_pos.w, 8*crsr_pos.y+crsr_pos.h, 7)
 else 
  rect(crsr_pos.x*8, crsr_pos.y*8, 8*crsr_pos.x+crsr_pos.w, 8*crsr_pos.y+crsr_pos.h, 15)
 end
end

function draw_instruction(instruction, i)
 row = flr((i-1)/10)
 col = ((i-1)-(row*10))
 spr(instruction+15, 32 + col*8, 81+row*8)
end


-- running the program
function update_running()

end

function draw_running()

end

-- completed the level
function update_completed()

end


-- {'u', 'd', 'l', 'r', 'tl', 'tr', 'f', 'h', 'lo', 'lc', 'co', 'cc', 'f1', 'f2'}
function draw_completed()
 
end



-- library functions
--- center align from: pico-8.wikia.com/wiki/centering_text
function hcenter(s)
 -- string length time   s the 
 -- pixels in a char's width
 -- cut in half and rounded down
 return (screen.w / 2)-flr((#s*4)/2)
end

function vcenter(s)
 -- string char's height
 -- cut in half and rounded down
 return (screen.h /2)-flr(5/2)
end


